"/category-products/:categoryCheck\" component={Categoryproduct} exact={true}/>\n      <Route path=\"/checkout/:orderId\" component={Checkout} exact={true}/>\n      <Route path=\"/product/:productId\" component={Productdetail} exact={true}/>\n      <Route path=\"/test\" component={Test} exact={true}/>\n      </div>\n     \n      </Switch>\n      \n    </div>\n  );\n}\nvar actions = {\n  firebaseAuthListener\n}\nexport default connect(null, actions)(App);\n","D:\\Code\\React\\Ecommerce\\code\\src\\Pages\\Home\\home.jsx",["198"],"import React from 'react'\r\nimport { auth } from '../../Firebase/firebase';\r\nimport './home.css'\r\n\r\nconst Home = () => {\r\n    return (\r\n        <div className='home'>\r\n            <div className=\"homeLeft flex\"><h1>home left</h1></div>\r\n            <div className=\"homeRight flex\"><h1>home right</h1></div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Home\r\n","D:\\Code\\React\\Ecommerce\\code\\src\\Pages\\Checkout\\checkout.js",[],"D:\\Code\\React\\Ecommerce\\code\\src\\Pages\\Authetication\\auth.js",[],"D:\\Code\\React\\Ecommerce\\code\\src\\Components\\CheckoutList\\checkoutlist.jsx",[],"D:\\Code\\React\\Ecommerce\\code\\src\\Utility\\checkout.js",[],"D:\\Code\\React\\Ecommerce\\code\\src\\Components\\Paragraphs\\paragraphs.jsx",[],"D:\\Code\\React\\Ecommerce\\code\\src\\Components\\SignupForm\\signupform.jsx",[],"D:\\Code\\React\\Ecommerce\\code\\src\\Components\\CheckoutListItems\\checkoutlistitems.jsx",[],"D:\\Code\\React\\Ecommerce\\code\\src\\Components\\CategoryListItems\\categoryListItems.jsx",[],"D:\\Code\\React\\Ecommerce\\code\\src\\Firebase\\firebase.jsx",[],"D:\\Code\\React\\Ecommerce\\code\\src\\Components\\SigninForm\\signin.jsx",[],"D:\\Code\\React\\Ecommerce\\code\\src\\Components\\Header\\header.jsx",[],"D:\\Code\\React\\Ecommerce\\code\\src\\Redux\\Authentication\\authActions.js",["199"],"import {\r\n  auth,\r\n  firestore,\r\n  serverTimestamp,\r\n  googleAuthProvider,\r\n} from \"../../Firebas